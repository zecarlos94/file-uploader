{
  "name": "file-uploader-rest-service",
  "description": "The backend REST service that handles requests to upload files to filesystem",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "backend",
    "csv files upload",
    "files upload"
  ],
  "contributors": [],
  "directories": {
    "config": "config",
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": "^18.13.0",
    "npm": ">= 8.19.3"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc --p ./tsconfig.json --outDir dist",
    "dev": "concurrently \"npm run generate-swagger-spec\" \"npm run watch\"",
    "generate-swagger-spec": "tsoa spec",
    "lint:fix": "eslint src/. tests/. --config .eslintrc --ext .ts --fix",
    "serve": "node dist",
    "start": "npm run generate-swagger-spec && npm run build && node dist",
    "tests": "jest --config ./jest.config.json --no-cache --silent",
    "tests:clear": "jest --clearCache --silent && rimraf file-uploads-tests/*",
    "tests:coverage": "jest --config ./jest.config.json --coverage --no-cache --silent",
    "tests:watch": "jest --config ./jest.config.json --no-cache --silent --watchAll",
    "watch": "ts-node-dev --trace-warnings -r tsconfig-paths/register --no-notify src/"
  },
  "types": "dist/",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "body-parser": "^1.20.2",
    "bytes": "^3.1.2",
    "cockatiel": "^3.1.2",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.5",
    "express": "^4.18.2",
    "express-prom-bundle": "6.6.0",
    "express-rate-limit": "^7.1.5",
    "http-graceful-shutdown": "^3.1.13",
    "http-status-codes": "^2.3.0",
    "jsonschema": "^1.4.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "os-utils": "^0.0.14",
    "prom-client": "^14.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "tsyringe": "^4.8.0",
    "ua-parser-js": "^1.0.37",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/config": "3.3.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/os-utils": "^0.0.4",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-path": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.3.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
